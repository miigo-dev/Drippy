<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:visibility="visible"
    tools:context=".Homepage">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="1dp"
        android:alpha="0.8"
        android:background="@drawable/bghomepage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="290dp"
        android:layout_height="146dp"
        android:layout_marginStart="61dp"
        android:layout_marginTop="384dp"
        android:fontFamily="@font/garetbold"
        android:text="@string/Find_your_style"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="268dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="556dp"
        android:fontFamily="@font/garet"
        android:text="@string/description"
        android:textAlignment="center"
        android:textColor="#D6D3D3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button"
        android:layout_width="250dp"
        android:layout_height="52dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="684dp"
        android:background="@drawable/inputbg"
        android:backgroundTint="@color/black"
        android:fontFamily="@font/garetbold"
        android:text="@string/started"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/ibSelling"
        android:layout_width="73dp"
        android:layout_height="59dp"
        android:layout_marginStart="328dp"
        android:layout_marginTop="27dp"
        android:background="?attr/selectableItemBackground"
        android:contentDescription="iconSelling"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sellingicon" />


</androidx.constraintlayout.widget.ConstraintLayout>