<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:visibility="visible"
    tools:context=".Homepage">

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-1dp"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="1dp"
        android:alpha="0.8"
        android:background="@drawable/bghomepage" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="290dp"
        android:layout_height="146dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="60dp"
        android:layout_marginTop="321dp"
        android:fontFamily="@font/garetbold"
        android:text="@string/Find_your_style"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="50sp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="268dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="71dp"
        android:layout_marginTop="472dp"
        android:fontFamily="@font/garet"
        android:text="@string/description"
        android:textAlignment="center"
        android:textColor="#D6D3D3" />

    <Button
        android:id="@+id/btnGetStarted"
        android:layout_width="251dp"
        android:layout_height="52dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="79dp"
        android:layout_marginTop="636dp"
        android:background="@drawable/inputbg"
        android:backgroundTint="@color/black"
        android:text="@string/started"
        android:fontFamily="@font/garetbold"
        android:textColor="@color/white" />

    <ImageButton
        android:id="@+id/ibSelling"
        android:layout_width="73dp"
        android:layout_height="59dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="328dp"
        android:layout_marginTop="27dp"
        android:background="?attr/selectableItemBackground"
        android:contentDescription="iconSelling"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/sellingicon" />


</RelativeLayout>