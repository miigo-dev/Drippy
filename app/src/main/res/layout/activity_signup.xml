<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FBFBFB"
    tools:context=".Signup"
    tools:layout_editor_absoluteX="-12dp"
    tools:layout_editor_absoluteY="-1dp">

    <ImageView
        android:id="@+id/passBg"
        android:layout_width="325dp"
        android:layout_height="63dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="444dp"
        android:src="@drawable/inputbg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textViewAgree"
        android:layout_width="208dp"
        android:layout_height="36dp"
        android:fontFamily="@font/garet"
        android:text="@string/txtTerms"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.674"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.693" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="40sp"
        android:layout_height="40sp"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.692"
        tools:ignore="TouchTargetSizeCheck" />

    <ImageView
        android:id="@+id/drippyLogo"
        android:layout_width="201dp"
        android:layout_height="219dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.116"
        app:srcCompat="@drawable/logo_round_border" />

    <Button
        android:id="@+id/btnCreate"
        android:layout_width="224dp"
        android:layout_height="44dp"
        android:background="@drawable/inputbg"
        android:fontFamily="@font/garetbold"
        android:text="@string/txtCreate"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.876"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/textViewSignIn"
        android:layout_width="282dp"
        android:layout_height="20dp"
        android:fontFamily="@font/garet"
        android:text="@string/txtSignIn"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.925" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="325dp"
        android:layout_height="63dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="372dp"
        android:src="@drawable/inputbg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="325dp"
        android:layout_height="63dp"
        android:layout_marginStart="43dp"
        android:layout_marginTop="300dp"
        android:src="@drawable/inputbg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="45dp"
        android:layout_height="44dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="310dp"
        android:src="@drawable/accounticon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="380dp"
        android:src="@drawable/accounticon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="44dp"
        android:layout_height="45dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="452dp"
        android:src="@drawable/passwordicon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="210dp"
        android:layout_height="48dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="308dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/garetbold"
        android:textColor="@color/white"
        android:hint="@string/txtUsername"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="210dp"
        android:layout_height="48dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="380dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/garetbold"
        android:textColor="@color/white"
        android:hint="@string/txtEmail"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="210dp"
        android:layout_height="48dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="452dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/garetbold"
        android:textColor="@color/white"
        android:hint="@string/txtPass"
        android:inputType="textPassword"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>